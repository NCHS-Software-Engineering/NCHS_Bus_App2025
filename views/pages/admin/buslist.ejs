<!DOCTYPE html>
<html>
    <head>
        <title>NCHS Bus Changes - Admin</title>

        <link rel="icon" type="image/png" href="/images/Naperville_Central_Logo.png">

        <!-- Latest compiled and minified CSS -->
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet">

        <!-- Latest compiled JavaScript -->
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>

        <link rel="stylesheet" href="/css/buslist.css">
        <link rel="stylesheet" href="/css/navbar.css">
        <link rel="stylesheet" href="/css/maincontent.css">
    </head>

    <body>
        <div id = "navbar">
            <button id="busNavButton" onclick = "window.location.href = '/buslist'"><image id="BusNavPicture" src = "/images/buslist.png"></image></button> <!--Redirects to Bus List-->
            <button onclick = "window.location.href = '/buschanges'"><image src = "/images/buschange.png"></image></button> <!--Redirects to Bus Changes-->
            <button onclick = "window.location.href = '/settings'" id = "active"><image src = "/images/settings.png"></image></button> <!--Redirects to Settings-->
            <button onclick = "window.location.replace('https://nchsbusapp.org')"><image src = "/images/exit.png"></image></button> <!--Exits Admin Panel-->
        </div>
        <div id="maincontent">
            <!-- Bus Buttons Section -->
            <div id="allBusses">
                <!-- Buttons will be dynamically added here -->
            </div>
        </div>
    </body>

    <script src="/js/admin/buslist.js"></script>
</html>
